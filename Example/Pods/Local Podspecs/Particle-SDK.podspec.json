  "homepage": "https://github.com/nielubowicz/Particle-SDK",
  "license": "MIT",
  "authors": {
    "Chris Nielubowicz": "nielubowicz@gmail.com"
  },
  "source": {
    "git": "https://github.com/nielubowicz/Particle-SDK.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
